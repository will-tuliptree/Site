.summary-container
    position relative
    height 630px
    .content
        position absolute
        top 0
        margin 0 auto
        text-align center
        width 100%
    h1
        font-size 26px
        margin-top 25px
        line-height 30px
        font-weight 700
        color $tuliptree-Aqua
    p
        margin-top 25px
        line-height 23px
        font-size 20px
        padding 0 2%
        font-style italic
        &:after
            content "\f107"
            display block
            font normal normal normal 14px/1 FontAwesome
            font-size 37px
            margin-top 10px


